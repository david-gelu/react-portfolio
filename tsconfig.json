{
  "compilerOptions": {
    "jsx": "react",
    "allowSyntheticDefaultImports": true,
    "baseUrl": ".",
    "esModuleInterop": false,
    "module": "commonjs",
    "moduleResolution": "node",
    "noImplicitAny": true,
    "outDir": "dist",
    "resolveJsonModule": true,
    "sourceMap": true,
    "target": "es2019",
    "ignoreDeprecations": "5.0",
  },
  "include": [
    "./src/**/*",
    "reportWebVitals.ts",
    "src/data.ts"
  ],
  "exclude": [
    "./node_modules",
  ]
}